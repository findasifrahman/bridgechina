{
  "buildCommand": "cd ../.. && pnpm install && pnpm --filter @bridgechina/web build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "cd ../.. && pnpm install",
  "framework": "vite",
  "devCommand": "cd ../.. && pnpm --filter @bridgechina/web dev",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}

